{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:makinacorpus/generated-hydrator-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pounard/goat-query-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pounard/goat-mapper.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^2.10",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "jbroadway/urlify": "^1.1",
        "makinacorpus/calista-core": "^1.0@alpha",
        "makinacorpus/filechunk-bundle": "^2.0.0-alpha8",
        "makinacorpus/generated-hydrator-bundle": "^1.0.0-alpha1",
        "makinacorpus/goat-mapper": "dev-master#0605275",
        "makinacorpus/goat-query": "^2.1.1",
        "makinacorpus/goat-query-bundle": "dev-master",
        "neitanod/forceutf8": "^2.0",
        "ocramius/generated-hydrator": "^3.1",
        "ocramius/package-versions": "^1.8",
        "ocramius/proxy-manager": "^2.8",
        "ramsey/uuid": "^3.8",
        "symfony/console": "5.1.*",
        "symfony/event-dispatcher": "5.1.*",
        "symfony/flex": "^1.1",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-foundation": "5.1.*",
        "symfony/http-kernel": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "5.1.*"
    },
    "config": {
        "platform": {
            "php": "7.4.5"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Goat\\Hydrator\\" : "vendor/makinacorpus/goat/src/Hydrator/",
            "GeneratedHydrator\\Bridge\\Symfony\\": "vendor/makinacorpus/generated-hydrator-bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "require-dev": {
        "doctrine/sql-formatter": "^1.0",
        "jdorn/sql-formatter": "^1.2"
    }
}
